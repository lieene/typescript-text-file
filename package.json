{
  "name": "text-editing",
  "version": "1.0.5",
  "description": "text file accessor",
  "main": "out/index.js",
  "types": "out/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "update": "npm i @lieene/ts-utility",
    "fastpatch": "git add . && git commit && npm version patch && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lieene/typescript-text-file.git"
  },
  "keywords": [
    "typescript",
    "text",
    "line",
    "colum",
    "edit"
  ],
  "author": "lieene",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lieene/typescript-text-file/issues"
  },
  "homepage": "https://github.com/lieene/typescript-text-file#readme",
  "devDependencies": {
    "@types/jest": "^24.0.22",
    "@types/node": "^12.12.8",
    "@types/vscode": "^1.40.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.2"
  },
  "files": [
    "out/**/*"
  ],
  "dependencies": {
    "@lieene/ts-utility": "^1.3.9"
  }
}
